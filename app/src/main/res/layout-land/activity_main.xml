        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.019" />

    <Button
        android:id="@+id/bCosNAcos"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
