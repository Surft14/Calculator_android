        android:onClick="onClickBExpNFactorial"
        android:text="@string/exp"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/bPiNPowCube"
        app:layout_constraintStart_toEndOf="@+id/bPiNPowCube"
        app:layout_constraintTop_toBottomOf="@+id/bPow2NCoshPowMin1"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/bFractionNTanh"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
